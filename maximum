import java.util.*;
public class goklul{
public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    
    int a[]=new int[n];
    
    for(int i=0;i<a.length;i++){
     
          
       int a[i]=sc.nextInt();
        int  max=a[0];
     if(max<a[i]){
         max=a[i];
     }}
      System.out.println(max);

}
    
}
